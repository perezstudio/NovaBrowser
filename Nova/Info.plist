<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>Nova needs camera access for video calls and web conferencing.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Nova needs microphone access for audio calls and web conferencing.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Nova needs Bluetooth access for wireless audio devices.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Nova needs Bluetooth access for wireless audio devices.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Some websites may request your location for location-based services.</string>
	<key>com.apple.security.get-task-allow</key>
	<true/>
	<key>com.apple.security.cs.disable-library-validation</key>
	<true/>
	<key>LSEnvironment</key>
	<dict>
		<key>WEBKIT_INSPECTOR_SERVER</key>
		<string>127.0.0.1:9999</string>
		<key>WEBKIT_SHOW_INSPECTOR</key>
		<string>1</string>
	</dict>
</dict>
</plist>