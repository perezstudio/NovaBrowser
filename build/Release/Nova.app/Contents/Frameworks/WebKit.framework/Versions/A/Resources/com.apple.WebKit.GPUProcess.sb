(define (system-graphics)
    (allow user-preference-read
        (preference-domain "com.apple.opengl")
        (preference-domain "com.nvidia.OpenGL"))
    (allow mach-lookup
        (global-name "com.apple.cvmsServ"))
    (allow file-read*
        (prefix "/private/var/db/CVMS/cvmsCodeSignObj"))
    (allow iokit-open-service
        (iokit-connection "IOAccelerator"))
    (if (equal? (param "ENABLE_SANDBOX_MESSAGE_FILTER") "YES")
        (allow iokit-open-user-client
            (iokit-connection "IOAccelerator")
            (apply-message-filter
                (allow (with report) (with telemetry)
                    iokit-async-external-method
                    iokit-external-method
                    iokit-external-trap)
                (when (not (equal? (param "CPU") "arm64"))
                    (allow iokit-external-method
                        (iokit-method-number 1 4 9 11 12 20 24 257 267))
                    (allow iokit-async-external-method
                        (iokit-method-number 0)))
                (allow iokit-external-method
                    (iokit-method-number 0 2 5 6 7 8 9 10 14 15 16 17 28 29 30 31 39 40 256 258 261 263 264))
                (with-filter (iokit-registry-entry-class "AGXDeviceUserClient")
                    (allow iokit-external-method
                        (iokit-method-number 21 22 23 24 259 260 262 270)))
                (with-filter (iokit-registry-entry-class "AppleParavirtDeviceUserClient")
                    (allow iokit-external-trap
                        (iokit-method-number 257 260 261))
                    (allow iokit-external-method
                        (iokit-method-number 257 259 260 262 265 266)))
                (allow iokit-external-trap
                    (iokit-trap-number 0 1 3))
                (allow iokit-async-external-method
                    (iokit-method-number 263))))
        (allow iokit-open-user-client
            (iokit-connection "IOAccelerator")))
    (allow iokit-open-user-client
        (iokit-user-client-class "IOSurfaceRootUserClient"))
    (deny iokit-open-user-client
        (iokit-user-client-class "IOAccelerationUserClient"))
    (deny iokit-open-user-client
        (iokit-user-client-class "IOFramebufferSharedUserClient"))
    (if (equal? (param "ENABLE_SANDBOX_MESSAGE_FILTER") "YES")
        (allow iokit-open-user-client
            (iokit-user-client-class "AppleIntelMEUserClient")
            (apply-message-filter
                (allow (with telemetry)
                    iokit-async-external-method
                    iokit-external-method
                    iokit-external-trap)
                (allow iokit-external-method
                    (iokit-method-number 1 2 3 4 5 6 114 115 120 132 133 144))))
        (allow iokit-open-user-client
            (iokit-user-client-class "AppleIntelMEUserClient")))
    (if (equal? (param "ENABLE_SANDBOX_MESSAGE_FILTER") "YES")
        (allow iokit-open-user-client (with report) (with telemetry)
            (iokit-user-client-class "AppleSNBFBUserClient")
            (apply-message-filter
                (allow (with telemetry)
                    iokit-async-external-method
                    iokit-external-method
                    iokit-external-trap)))
        (allow iokit-open-user-client
            (iokit-user-client-class "AppleSNBFBUserClient")))
    (deny iokit-open-user-client (with no-report)
        (iokit-user-client-class "AGPMClient")
        (iokit-user-client-class "AppleGraphicsControlClient")
        (iokit-user-client-class "AppleGraphicsPolicyClient"))
    (deny iokit-open-user-client
